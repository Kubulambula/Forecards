; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Control",
"class": "Card",
"language": "GDScript",
"path": "res://scripts/cards/card.gd"
} ]
_global_script_class_icons={
"Card": ""
}

[application]

config/name="Weather"
run/main_scene="res://scenes/ui/MainMenu.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Weather"
config/icon="res://icon.png"

[autoload]

WeatherData="*res://scripts/singletons/weather_data.gd"
ResourceQueue="*res://scripts/singletons/resource_queue.gd"

[display]

window/size/width=1280
window/size/height=720
window/size/resizable=false
window/dpi/allow_hidpi=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
